{
  "name": "udkme-web",
  "version": "0.0.1",
  "description": "Front-end for You Don't Know Me. Hack Upstate Fall 2015.",
  "directories": {
    "test": "./src/js/test"
  },
  "scripts": {
    "clean": "rm -rf build/*",
    "lint": "eslint src",
    "test": "mocha ./src/js/test --compilers js:babel/register --recursive --require ./src/js/test/setup.js",
    "build": "npm run clean && webpack --progress --colors --config config/webpack.production.js",
    "build-dev": "npm run clean && webpack --progress --colors --watch --config config/webpack.development.js",
    "dev": "npm run clean && nodemon --exec babel-node --stage 0 -- dev-server.js",
    "watch": "npm run build-dev",
    "postinstall": "NODE_ENV=production DEPLOY=heroku npm run build",
    "start": "babel-node --stage 0 -- server.js",
    "dev-server": "npm run watch && python ../webapp/app.py"
  },
  "engines": {
    "node": "4.0.0",
    "npm": "2.14.2"
  },
  "author": "Billy Ceskavich",
  "repository": "https://github.com/bceskavich/udkme-web",
  "dependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.8.23",
    "babel-loader": "^5.3.1",
    "config": "^1.15.0",
    "css-loader": "^0.15.2",
    "css-patterns": "^0.2.0",
    "eslint-loader": "^1.0.0",
    "express": "^4.13.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "history": "^1.9.0",
    "html-webpack-plugin": "^1.6.0",
    "lodash": "^3.10.1",
    "node-sass": "^3.2.0",
    "nodemon": "^1.3.7",
    "proxy-middleware": "^0.13.1",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.2.8",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc1",
    "redux": "^3.0.3",
    "redux-logger": "^2.0.4",
    "redux-multi": "^0.1.9",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "request": "^2.58.0",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.1",
    "webpack-config-merger": "0.0.3",
    "webpack-dev-server": "^1.10.1"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.23",
    "eslint": "^1.0.0-rc-1",
    "eslint-plugin-react": "^2.7.1",
    "expect": "^1.12.2",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.3",
    "sinon": "^1.17.1"
  }
}
